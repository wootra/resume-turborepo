{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "module": "ES2022",
        "baseUrl": ".",
        "jsxImportSource": "react",
        "jsx": "preserve",
        "paths": {
            "@/*": ["src/*"]
        }
    },
    "include": [
        "./*.d.ts",
        "src/**/*.d.ts",
        "src/**/*.ts",
        "src/**/*.tsx",
        "src/**/*.astro"
    ]
}
