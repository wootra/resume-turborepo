---
import 'resume-css/variables/variables.css';// root
import { FrameworkNoticeBanner } from 'solid-ui';
const sourceLinks={
	"source repo": "https://github.com/wootra/resume-turborepo/tree/main/apps/astro-resume"
}
import type { bannerLinks } from 'common-data';
const selected: keyof typeof bannerLinks = 'Astro JS(Solid.js)';
export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Songhyeon Jun's resume in astro">
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<link href="https://fonts.googleapis.com/css2?family=Oswald:wght@500&amp;family=Roboto+Slab&amp;display=swap" rel="stylesheet">
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body>
		<FrameworkNoticeBanner description="This page is written in Astro.js, Solid.js, CSS3, and Tailwind" currentLabel={selected} sourceLinks={sourceLinks} client:load />
		<slot />
	</body>
</html>
<style is:global>
	:root {
		
	}
	html {
		font-family: system-ui, sans-serif;
		background-color: #F6F6F6;
	}

	body {
		margin: 0;
    	min-width: 381px;
	}
	code {
		font-family: Menlo, Monaco, Lucida Console, Liberation Mono, DejaVu Sans Mono,
			Bitstream Vera Sans Mono, Courier New, monospace;
	}

	
</style>
